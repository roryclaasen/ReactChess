	"bugs": {
		"url": "https://github.com/roryclaasen/ReactChess/issues"
	},
	"dependencies": {
		"@material-ui/core": "^3.2.2",
		"@material-ui/icons": "^3.0.1",
		"es6-promise": "^4.2.5",
		"express": "^4.16.4",
		"normalize.css": "^8.0.0",
		"npm-run-all": "^4.1.3",
		"prop-types": "^15.6.2",
		"react": "^16.5.2",
		"react-app-rewire-scss": "^1.0.2",
		"react-app-rewire-typescript": "^2.0.2",
		"react-app-rewired": "^1.6.2",
		"react-dnd": "^5.0.0",
		"react-dnd-html5-backend": "^5.0.1",
		"react-dom": "^16.5.2",
		"react-github-corner": "^2.3.0",
		"react-scripts": "2.0.5",
		"socket.io": "^2.1.1",
		"socket.io-client": "^2.1.1",
		"ts-loader": "^4.0.0",
		"ts-node": "^7.0.1",
		"typescript": "^3.1.3"
	},
	"devDependencies": {
		"@types/express": "^4.16.0",
		"@types/node-localstorage": "^1.3.0",
		"@types/react-dom": "^16.0.9",
		"@types/socket.io": "^2.1.0",
		"@types/socket.io-client": "^1.4.32",
		"eslint-config-airbnb": "^17.1.0",
		"sass-lint": "^1.12.1",
		"tslint": "^5.11.0",
		"tslint-config-airbnb": "^5.11.0",
		"tslint-eslint-rules": "^5.4.0",
		"tslint-react": "^3.6.0"
	},
	"scripts": {
		"start": "node dist/server",
		"start:client": "react-app-rewired start",
		"start:server": "ts-node --project tsconfig.server.json src/server",
		"build": "run-s build:client build:server",
		"build:client": "react-app-rewired build",
		"build:server": "tsc --project tsconfig.server.json",
		"lint": "run-s lint:sass lint:ts",
		"lint:sass": "sass-lint -v",
		"lint:js": "eslint src/**/*.js",
		"lint:ts": "run-s lint:ts:shared lint:ts:client lint:ts:server",
		"lint:ts:shared": "tslint --project tsconfig.json",
		"lint:ts:client": "tslint --project tsconfig.client.json",
		"lint:ts:server": "tslint --project tsconfig.server.json"
	},
	"browserslist": [
		">0.2%",
		"not dead",
		"not ie <= 11",
		"not op_mini all"
	]
}
